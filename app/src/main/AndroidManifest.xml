<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whp.quizlocal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.whp.quizlocal.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.whp.quizlocal.activity.GameListActivity">
        </activity>
        <activity android:name="com.whp.quizlocal.activity.GameBriefActivity">
        </activity>
        <activity android:name="com.whp.quizlocal.activity.QuizActivity">
        </activity>
        <activity android:name="com.whp.quizlocal.activity.GameResultsActivity">
        </activity>


    </application>

</manifest>